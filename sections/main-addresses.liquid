<link rel="stylesheet" href="{{ 'opk-addresses.css' | asset_url }}" type="text/css">

{% assign header_image = settings.customer_header_image %}
{% assign header_heading = 'customer.addresses.title' | t %}
{% assign show_breadcrumbs = settings.show_customer_breadcrumbs %}
{% assign show_heading = settings.show_customer_heading %}

{% assign image_overlay_color = settings.customer_image_overlay_color %}
{% assign image_overlay_opacity = settings.customer_image_overlay_opacity %}

{% if header_image != blank %}
    {% render 'custom_page_header' image: header_image, heading: header_heading, show_breadcrumbs: show_breadcrumbs, show_heading: show_heading, image_overlay_color: image_overlay_color, image_overlay_opacity: image_overlay_opacity %}
{% endif %}

<section id="section-{{ section.id }}" data-section-id="{{ section.id }}" class='opk-addresses'>
    {% paginate customer.addresses by 5 %}

        <div class="page-width-small opk-addresses__content-container">
            {% if show_breadcrumbs and header_image == blank %}
                <div class='account-breadcrumbs'>
                    <ol class="breadcrumbs__list">
                        <li class="breadcrumbs__item">
                            <a class="breadcrumbs__link"
                               href="{{ routes.root_url }}">{{ 'general.breadcrumbs.home' | t }}</a>
                        </li>
                        <li class="breadcrumbs__item">
                            <span>{{ 'customer.order.title' | t: name: order.name }}</span>
                        </li>
                    </ol>
                </div>
            {% endif %}

            <header class="section-header text-left opk-addresses__header">
                {% if show_heading and header_image == blank %}
                    <h1 class="text-center h3 opk-addresses__title head_title__text_bg head_title__word">{{ 'customer.addresses.title' | t }}</h1>
                {% endif %}

                <div class='return_with_icon_link__row opk-addresses__navigation'>
                    <a href="{{ routes.account_url }}"
                       class='link--primary return_with_icon_link opk-addresses__navigation-item'>
                        {% render 'icon' with 'tail-left' %}
                        <span> {{ 'customer.account.return' | t }}</span>
                    </a>

                    <span id="AddressNewButton" aria-expanded="false" aria-owns="AddressNewForm"
                          class='link--primary return_with_icon_link address-new-toggle opk-addresses__navigation-item'>
                        {{ 'customer.addresses.add_new' | t }}
                    </span>
                </div>
            </header>
        </div>

        <div class="page-width-small opk-addresses__content-container">
            <div class="grid opk-addresses__grid">
                <div class="grid__item opk-addresses__grid-item {% if customer.addresses.size == 0 %}opk-addresses__grid-item--empty{% endif %}">
                    <div id="AddressNewForm" class="form-vertical hide opk-addresses__form-wrapper">
                        {% form 'customer_address', customer.new_address, class: 'customer-address-form opk-addresses__form' %}
                            <div class='customer-address-form--header opk-addresses__form-header'>
                                <h4 class="text-center h3 opk-addresses__title head_title__text_bg head_title__word">{{ 'customer.addresses.add_new' | t }}</h4>
                                <p class="opk-addresses__subtitle">Account information</p>
                            </div>

                            <div class="grid">
                                <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                    <label for="AddressFirstNameNew"
                                           class="opk-addresses__label">{{ section.settings.add_first_name_placeholder }}
                                        *</label>
                                    <input type="text" id="AddressFirstNameNew" name="address[first_name]"
                                           value="{{ form.first_name }}" autocomplete="given-name"
                                           placeholder="{{ 'customer.addresses.first_name' | t }}">
                                </div>

                                <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                    <label for="AddressLastNameNew"
                                           class="opk-addresses__label">{{ section.settings.add_last_name_placeholder }}
                                        *</label>
                                    <input type="text" id="AddressLastNameNew" name="address[last_name]"
                                           value="{{ form.last_name }}" autocomplete="family-name"
                                           placeholder="{{ 'customer.addresses.last_name' | t }}">
                                </div>
                            </div>

                            <div class="grid">
                                <div class="grid__item medium-up--one-half opk-addresses__input-wrapper grid__item">
                                    <label for="AddressAddress1New"
                                           class="opk-addresses__label">{{ section.settings.add_address1_placeholder }}
                                        *</label>
                                    <input type="text" id="AddressAddress1New" name="address[address1]"
                                           value="{{ form.address1 }}" autocomplete="street-address address-line1"
                                           placeholder="{{ 'customer.addresses.address1' | t }}">
                                </div>

                                <div class="grid__item medium-up--one-half opk-addresses__input-wrapper grid__item">
                                    <label for="AddressAddress2New"
                                           class="opk-addresses__label">{{ section.settings.add_address2_placeholder }}
                                        *</label>
                                    <input type="text" id="AddressAddress2New" name="address[address2]"
                                           value="{{ form.address2 }}" autocomplete="street-address address-line2"
                                           placeholder="{{ 'customer.addresses.address2' | t }}">
                                </div>
                            </div>


                            <div class="grid">
                                <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                    <label for="AddressCityNew"
                                           class="opk-addresses__label">{{ section.settings.add_city_placeholder }}
                                        *</label>
                                    <input type="text" id="AddressCityNew" name="address[city]"
                                           value="{{ form.city }}"
                                           autocomplete="address-level2"
                                           placeholder="{{ 'customer.addresses.city' | t }}"/>

                                </div>

                                <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                    <label for="AddressCountryNew"
                                           class="opk-addresses__label">{{ section.settings.add_country_placeholder }}
                                        *</label>
                                    <select id="AddressCountryNew" name="address[country]"
                                            data-default="Netherlands"
                                            autocomplete="country">
                                        {{ all_country_option_tags | remove: '<option value="Netherlands" data-provinces="[]">Netherlands</option>' | replace: '<option value="---" data-provinces="[]">---</option>', ' <option selected value="Netherlands" data-provinces="[]">Netherlands</option>' }}
                                    </select>
                                </div>
                            </div>

                            <div class="  opk-addresses__input-wrapper">
                                <label for="AddressZipNew"
                                       class="opk-addresses__label">{{ section.settings.add_zip_placeholder }}</label>
                                <input type="text" id="AddressZipNew" name="address[zip]" value="{{ form.zip }}"
                                       autocapitalize="characters" autocomplete="postal-code"
                                       placeholder="{{ 'customer.addresses.zip' | t }}">
                            </div>

                            <div class=" opk-addresses__input-wrapper">
                                <label for="AddressPhoneNew"
                                       class="opk-addresses__label">{{ section.settings.add_phone_placeholder }}</label>
                                <input type="tel" id="AddressPhoneNew" name="address[phone]"
                                       value="{{ form.phone }}" autocomplete="phone"
                                       placeholder="{{ 'customer.addresses.phone' | t }}">
                            </div>

                            <div class="text-left opk-addresses__form-footer">
                                {{ form.set_as_default_checkbox }}
                                <label for="address_default_address_new">{{ 'customer.addresses.set_default' | t }}</label>

                                <div class='address_buttons opk-addresses__form-buttons'>
                                    <button type="submit"
                                            class="btn btn--primary btn--align-center opk-addresses__form-button"
                                            data-form-id="{{ form.id }}">
                                        <span>{{ 'customer.addresses.add' | t }}</span></button>
                                    <button type="reset"
                                            class='link--primary return_with_icon_link address-new-toggle opk-addresses__navigation-item'>
                                        {{ 'customer.addresses.cancel' | t }}
                                        {% render 'icon' with 'tail-right' %}
                                    </button>
                                </div>
                            </div>
                        {% endform %}
                    </div>

                    {% if customer.addresses.size > 0 %}
                        <ul class='addresses-list opk-addresses__addresses'>
                            <li class="opk-addresses__address text-left">
                                <div class="address">
                                    <div class="opk-addresses__default-marker">
                                        <h2 class=" opk-addresses__default-marker-title">{{ 'customer.addresses.default' | t }}</h2>
                                    </div>

                                    <h5 class="opk-addresses__text">{{ customer.default_address.name }}</h5>

                                    {% if customer.default_address.street != blank or customer.default_address.city != blank or customer.default_address.zip != blank %}
                                        <p class="opk-addresses__text">
                                            {% if customer.default_address.street != blank %}{{ customer.default_address.street }},{% endif %}
                                            {% if customer.default_address.city != blank %}{{ customer.default_address.city }},{% endif %}
                                            {% if customer.default_address.zip != blank %}{{ customer.default_address.zip }}{% endif %}
                                        </p>
                                    {% endif %}

                                    {% if customer.default_address.country != blank %}
                                        <h5 class="opk-addresses__text">
                                            {{ customer.default_address.country }}{% if customer.default_address.zip != blank %}{% endif %}
                                        </h5>
                                    {% endif %}

                                    <div class="address_buttons opk-addresses__buttons">
                                        <button
                                                type="button"
                                                class="address-delete btn  btn--primary btn--full btn--align-center link--primary opk-addresses__button opk-addresses__button--secondary"
                                                data-target="{{ customer.default_address.url }}"
                                                data-confirm-message="{{ 'customer.addresses.delete_confirm' | t }}"
                                        >
                                            <span> {{ 'customer.addresses.delete' | t }}</span>
                                        </button>
                                        <button
                                                id="EditFormButton_{{ customer.default_address.id }}"
                                                type="button"
                                                class="address-edit-toggle btn  btn--primary btn--full btn--align-center  link--primary opk-addresses__button"
                                                data-form-id="{{ customer.default_address.id }}"
                                                aria-owns="EditAddress_{{ customer.default_address.id }}"
                                                aria-expanded="false"
                                        >
                                            <span> {{ 'customer.addresses.edit' | t }}</span>
                                        </button>
                                    </div>
                                </div>

                                <div id="EditAddress_{{ customer.default_address.id }}"
                                     class="form-vertical hide opk-addresses__form-wrapper">
                                    {% form 'customer_address', customer.default_address, class: 'customer-address-form opk-addresses__form' %}
                                        <div class='customer-address-form--header opk-addresses__form-header'>
                                            <h4 class="text-center h3 opk-addresses__title head_title__text_bg head_title__word">{{ 'customer.addresses.edit_address' | t }}</h4>
                                            <p class="opk-addresses__subtitle">Account information</p>
                                        </div>

                                        <div class="grid">
                                            <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                                <label for="AddressFirstName_{{ form.id }}"
                                                       class="opk-addresses__label">{{ section.settings.edit_first_name_placeholder }}
                                                    *</label>
                                                <input type="text" id="AddressFirstName_{{ form.id }}"
                                                       name="address[first_name]" value="{{ form.first_name }}"
                                                       autocomplete="given-name"
                                                       placeholder="{{ 'customer.addresses.first_name' | t }}"
                                                       required>
                                            </div>

                                            <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                                <label for="AddressLastName_{{ form.id }}"
                                                       class="opk-addresses__label">{{ section.settings.edit_last_name_placeholder }}
                                                    *</label>
                                                <input type="text" id="AddressLastName_{{ form.id }}"
                                                       name="address[last_name]" value="{{ form.last_name }}"
                                                       autocomplete="family-name"
                                                       placeholder="{{ 'customer.addresses.last_name' | t }}"
                                                       required>
                                            </div>
                                        </div>

                                        <div class="grid">
                                            <div class="opk-addresses__input-wrapper grid__item medium-up--one-half">
                                                <label for="AddressAddress1_{{ form.id }}"
                                                       class="opk-addresses__label">{{ section.settings.edit_address1_placeholder }}
                                                    *</label>
                                                <input type="text" id="AddressAddress1_{{ form.id }}"
                                                       name="address[address1]"
                                                       value="{{ form.address1 }}"
                                                       autocomplete="street-address address-line1"
                                                       placeholder="{{ 'customer.addresses.address1' | t }}"
                                                       required>
                                            </div>

                                            <div class="opk-addresses__input-wrapper grid__item medium-up--one-half">
                                                <label for="AddressAddress2_{{ form.id }}"
                                                       class="opk-addresses__label">{{ section.settings.edit_address2_placeholder }}
                                                    *</label>
                                                <input type="text" id="AddressAddress2_{{ form.id }}"
                                                       name="address[address2]"
                                                       value="{{ form.address2 }}"
                                                       autocomplete="street-address address-line2"
                                                       placeholder="{{ 'customer.addresses.address2' | t }}"
                                                       required>
                                            </div>
                                        </div>

                                        <div class="grid">
                                            <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                                <label for="AddressCity_{{ form.id }}"
                                                       class="opk-addresses__label">{{ section.settings.edit_city_placeholder }}
                                                    *</label>
                                                <input type="text" id="AddressCity_{{ form.id }}"
                                                       name="address[city]"
                                                       value="{{ form.city }}" autocomplete="address-level2"
                                                       placeholder="{{ 'customer.addresses.city' | t }}"
                                                       required>
                                            </div>

                                            <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                                <label for="AddressCountry_{{ form.id }}"
                                                       class="opk-addresses__label">{{ section.settings.edit_country_placeholder }}
                                                    *</label>
                                                <select id="AddressCountry_{{ form.id }}"
                                                        class="address-country-option"
                                                        data-form-id="{{ form.id }}" name="address[country]"
                                                        data-default="{{ form.country }}"
                                                        autocomplete="country" required>
                                                    {% assign defaultCountry = '<option selected value="' | append: form.country | append: '" data-provinces="[]">' | append: form.country | append: '</option>' %}

                                                    {{ all_country_option_tags | replace: '<option value="---" data-provinces="[]">---</option>', defaultCountry }}
                                                </select>
                                            </div>
                                        </div>

                                        <div class="opk-addresses__input-wrapper">
                                            <label for="AddressZip_{{ form.id }}"
                                                   class="opk-addresses__label">{{ section.settings.edit_zip_placeholder }}
                                                *</label>
                                            <input type="text" id="AddressZip_{{ form.id }}" name="address[zip]"
                                                   value="{{ form.zip }}" autocapitalize="characters"
                                                   autocomplete="postal-code"
                                                   placeholder="{{ 'customer.addresses.zip' | t }}">
                                        </div>

                                        <div class="opk-addresses__input-wrapper">
                                            <label for="AddressPhone_{{ form.id }}"
                                                   class="opk-addresses__label">{{ section.settings.edit_phone_placeholder }}
                                                *</label>
                                            <input type="tel" id="AddressPhone_{{ form.id }}"
                                                   name="address[phone]"
                                                   value="{{ form.phone }}" autocomplete="phone"
                                                   placeholder="{{ 'customer.addresses.phone' | t }}">
                                        </div>


                                        <div class="text-left opk-addresses__form-footer">
                                            <input type="checkbox" id="address_default_address_{{ form.id }}"
                                                   name="address[default]" value="1" checked>
                                            <label for="address_default_address_{{ form.id }}">{{ 'customer.addresses.set_default' | t }}</label>

                                            <div class='address_buttons opk-addresses__form-buttons'>
                                                <button type="submit"
                                                        class="btn btn--primary btn--align-center opk-addresses__form-button"
                                                        data-form-id="{{ form.id }}"
                                                        aria-expanded="false"
                                                >
                                                    <span>{{ 'customer.addresses.update' | t }}</span></button>
                                                <a href="{{ full_url }}"
                                                   class='link--primary return_with_icon_link address-new-toggle opk-addresses__navigation-item'>
                                                    {{ 'customer.addresses.cancel' | t }}
                                                </a>
                                            </div>
                                        </div>
                                    {% endform %}
                                </div>
                            </li>

                            {% for address in customer.addresses %}
                                {% if customer.default_address.id != address.id %}
                                    <li class="opk-addresses__address text-left">
                                        <div class="address">
                                            <div class="opk-addresses__default-marker">
                                                {% if address == customer.default_address %}
                                                    <h2 class=" opk-addresses__default-marker-title">{{ 'customer.addresses.default' | t }}</h2>
                                                {% endif %}
                                            </div>

                                            <h5 class="opk-addresses__text">{{ address.name }}</h5>

                                            {% if address.street != blank or address.city != blank or address.zip != blank %}
                                                <p class="opk-addresses__text">
                                                    {% if address.street != blank %}{{ address.street }},{% endif %}
                                                    {% if address.city != blank %}{{ address.city }},{% endif %}
                                                    {% if address.zip != blank %}{{ address.zip }}{% endif %}
                                                </p>
                                            {% endif %}

                                            {% if address.country != blank %}
                                                <h5 class="opk-addresses__text">
                                                    {{ address.country }}{% if address.zip != blank %}{% endif %}
                                                </h5>
                                            {% endif %}

                                            <div class="address_buttons opk-addresses__buttons">
                                                <button type="button"
                                                        class="address-delete btn  btn--primary btn--full btn--align-center link--primary opk-addresses__button opk-addresses__button--secondary"
                                                        data-target="{{ address.url }}"
                                                        data-confirm-message="{{ 'customer.addresses.delete_confirm' | t }}"
                                                >
                                                    <span> {{ 'customer.addresses.delete' | t }}</span>
                                                </button>
                                                <button
                                                        id="EditFormButton_{{ address.id }}"
                                                        type="button"
                                                        class="address-edit-toggle btn  btn--primary btn--full btn--align-center  link--primary opk-addresses__button"
                                                        data-form-id="{{ address.id }}"
                                                        aria-owns="EditAddress_{{ address.id }}"
                                                        aria-expanded="false"
                                                >
                                                    <span> {{ 'customer.addresses.edit' | t }}</span>
                                                </button>
                                            </div>
                                        </div>

                                        <div id="EditAddress_{{ address.id }}"
                                             class="form-vertical hide opk-addresses__form-wrapper">
                                            {% form 'customer_address', address, class: 'customer-address-form opk-addresses__form' %}
                                                <div class='customer-address-form--header opk-addresses__form-header'>
                                                    <h4 class="text-center h3 opk-addresses__title head_title__text_bg head_title__word">{{ 'customer.addresses.edit_address' | t }}</h4>
                                                    <p class="opk-addresses__subtitle">Account information</p>
                                                </div>

                                                <div class="grid">
                                                    <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                                        <label for="AddressFirstName_{{ form.id }}"
                                                               class="opk-addresses__label">{{ section.settings.edit_first_name_placeholder }}
                                                            *</label>
                                                        <input type="text" id="AddressFirstName_{{ form.id }}"
                                                               name="address[first_name]" value="{{ form.first_name }}"
                                                               autocomplete="given-name"
                                                               placeholder="{{ 'customer.addresses.first_name' | t }}"
                                                               required>
                                                    </div>

                                                    <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                                        <label for="AddressLastName_{{ form.id }}"
                                                               class="opk-addresses__label">{{ section.settings.edit_last_name_placeholder }}
                                                            *</label>
                                                        <input type="text" id="AddressLastName_{{ form.id }}"
                                                               name="address[last_name]" value="{{ form.last_name }}"
                                                               autocomplete="family-name"
                                                               placeholder="{{ 'customer.addresses.last_name' | t }}"
                                                               required>
                                                    </div>
                                                </div>

                                                <div class="grid">
                                                    <div class="opk-addresses__input-wrapper grid__item medium-up--one-half">
                                                        <label for="AddressAddress1_{{ form.id }}"
                                                               class="opk-addresses__label">{{ section.settings.edit_address1_placeholder }}
                                                            *</label>
                                                        <input type="text" id="AddressAddress1_{{ form.id }}"
                                                               name="address[address1]"
                                                               value="{{ form.address1 }}"
                                                               autocomplete="street-address address-line1"
                                                               placeholder="{{ 'customer.addresses.address1' | t }}"
                                                               required>
                                                    </div>

                                                    <div class="opk-addresses__input-wrapper grid__item medium-up--one-half">
                                                        <label for="AddressAddress2_{{ form.id }}"
                                                               class="opk-addresses__label">{{ section.settings.edit_address2_placeholder }}
                                                            *</label>
                                                        <input type="text" id="AddressAddress2_{{ form.id }}"
                                                               name="address[address2]"
                                                               value="{{ form.address2 }}"
                                                               autocomplete="street-address address-line2"
                                                               placeholder="{{ 'customer.addresses.address2' | t }}"
                                                               required>
                                                    </div>
                                                </div>

                                                <div class="grid">
                                                    <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                                        <label for="AddressCity_{{ form.id }}"
                                                               class="opk-addresses__label">{{ section.settings.edit_city_placeholder }}
                                                            *</label>
                                                        <input type="text" id="AddressCity_{{ form.id }}"
                                                               name="address[city]"
                                                               value="{{ form.city }}" autocomplete="address-level2"
                                                               placeholder="{{ 'customer.addresses.city' | t }}"
                                                               required>
                                                    </div>

                                                    <div class="grid__item medium-up--one-half opk-addresses__input-wrapper">
                                                        <label for="AddressCountry_{{ form.id }}"
                                                               class="opk-addresses__label">{{ section.settings.edit_country_placeholder }}
                                                            *</label>
                                                        <select id="AddressCountry_{{ form.id }}"
                                                                class="address-country-option"
                                                                data-form-id="{{ form.id }}" name="address[country]"
                                                                data-default="{{ form.country }}"
                                                                autocomplete="country" required>
                                                            {% assign defaultCountry = '<option selected value="' | append: form.country | append: '" data-provinces="[]">' | append: form.country | append: '</option>' %}

                                                            {{ all_country_option_tags | replace: '<option value="---" data-provinces="[]">---</option>', defaultCountry }}
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="opk-addresses__input-wrapper">
                                                    <label for="AddressZip_{{ form.id }}"
                                                           class="opk-addresses__label">{{ section.settings.edit_zip_placeholder }}
                                                        *</label>
                                                    <input type="text" id="AddressZip_{{ form.id }}" name="address[zip]"
                                                           value="{{ form.zip }}" autocapitalize="characters"
                                                           autocomplete="postal-code"
                                                           placeholder="{{ 'customer.addresses.zip' | t }}">
                                                </div>

                                                <div class="opk-addresses__input-wrapper">
                                                    <label for="AddressPhone_{{ form.id }}"
                                                           class="opk-addresses__label">{{ section.settings.edit_phone_placeholder }}
                                                        *</label>
                                                    <input type="tel" id="AddressPhone_{{ form.id }}"
                                                           name="address[phone]"
                                                           value="{{ form.phone }}" autocomplete="phone"
                                                           placeholder="{{ 'customer.addresses.phone' | t }}">
                                                </div>


                                                <div class="text-left opk-addresses__form-footer">
                                                    {{ form.set_as_default_checkbox }}
                                                    <label for="address_default_address_{{ form.id }}">{{ 'customer.addresses.set_default' | t }}</label>

                                                    <div class='address_buttons opk-addresses__form-buttons'>
                                                        <button type="submit"
                                                                class="btn btn--primary btn--align-center opk-addresses__form-button"
                                                                data-form-id="{{ form.id }}"
                                                                aria-expanded="false">
                                                            <span>{{ 'customer.addresses.update' | t }}</span></button>

                                                        <a href="{{ full_url }}"
                                                           class='link--primary return_with_icon_link address-new-toggle opk-addresses__navigation-item'>
                                                            {{ 'customer.addresses.cancel' | t }}
                                                        </a>
                                                    </div>
                                                </div>
                                            {% endform %}
                                        </div>
                                    </li>
                                {% endif %}
                            {% endfor %}
                        </ul>

                    {% else %}
                        <p class="opk-addresses__empty-title">{{ section.settings.empty_text }}</p>

                    {%- endif -%}

                    {%- if paginate.pages > 1 -%}
                        {% render 'pagination', paginate: paginate %}
                    {%- endif -%}
                </div>
            </div>
        </div>
    {% endpaginate %}
</section>

<script type="module" src="{{ 'opk-addresses.js' | asset_url }}"></script>

{% schema %}
{
  "name": "t:sections.main-addresses.name",
  "settings": [
    {
      "type": "text",
      "id": "edit_first_name_placeholder",
      "label": "Edit First name placeholder",
      "default": "First name"
    },
    {
      "type": "text",
      "id": "edit_last_name_placeholder",
      "label": "Edit Last name placeholder",
      "default": "Last name"
    },
    {
      "type": "text",
      "id": "edit_address1_placeholder",
      "label": "Edit Address placeholder",
